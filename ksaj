(letrec   ([evenp      (lambda             (n          )
           (or         (zerop               n          )
           (oddp       (1-                  n      ))))]
           [oddp       (lambda             (n          )
           (and        (not      (zerop     n         ))
           (evenp      (1-                  n     ))))])
                       (oddp                        11))

